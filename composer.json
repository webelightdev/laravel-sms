{
    "name": "webelightdev/laravel-sms",
    "description": "package Sms Integration",
    "type": "composer-plugin",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Saifi Khambhati",
            "email": "saifik@webelight.com"
        }
    ],
    "require": {
        "twilio/sdk": "^5.19"
    },
    "autoload": {
        "psr-4": {       
            "Webelightdev\\LaravelSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webelightdev\\LaravelSms\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms" : "Webelightdev\\LaravelSms\\Facade"
            }
        }
    }
}
